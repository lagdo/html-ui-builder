{
    "name": "lagdo/html-ui-builder",
    "type": "library",
    "description": "UI Builder extensions for HTML/CSS frameworks.",
    "authors": [
        {
            "name": "Thierry Feuzeu",
            "email": "thierry.feuzeu@gmail.com"
        }
    ],
    "homepage": "https://github.com/lagdo/html-ui-builder",
    "license": "MIT",
    "keywords": [
        "HTML",
        "UI Builder",
        "Bootstrap"
    ],
    "support": {
        "issues": "https://github.com/lagdo/html-ui-builder/issues",
        "source": "https://github.com/lagdo/html-ui-builder"
    },
    "require": {
        "avplab/php-html-builder": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Lagdo\\UiBuilder\\": "ui-builder/src/",
            "Lagdo\\UiBuilder\\Bootstrap3\\": "ui-builder-bootstrap3/src/",
            "Lagdo\\UiBuilder\\Bootstrap4\\": "ui-builder-bootstrap4/src/",
            "Lagdo\\UiBuilder\\Bootstrap5\\": "ui-builder-bootstrap5/src/"
        },
        "files": [
            "ui-builder-bootstrap3/src/start.php",
            "ui-builder-bootstrap4/src/start.php",
            "ui-builder-bootstrap5/src/start.php"
        ]
    },
    "require-dev": {
        "contao/monorepo-tools": "^0.2"
    },
    "replace": {
        "lagdo/ui-builder": "self.version",
        "lagdo/ui-builder-bootstrap3": "self.version",
        "lagdo/ui-builder-bootstrap4": "self.version",
        "lagdo/ui-builder-bootstrap5": "self.version"
    },
    "extra": {
        "contao-manager-plugin": []
    }
}
